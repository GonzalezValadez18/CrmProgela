<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="start"
    android:id="@+id/rootLayout"
    tools:context=".clientes.ui.view.alta.Alta_Clientes_F1">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarAltaProspectos"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="130dp"/>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="30dp"
        android:layout_marginTop="30dp"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        android:background="@drawable/custom_edit_text"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal"
            android:padding="14dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_encabezado1"
                android:id="@+id/loginText"
                android:textSize="30sp"
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="@color/blue_progela"/>
            <EditText
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/txtRazonSocial"
                android:background="@drawable/custom_edit_text"
                android:drawableStart="@drawable/baseline_person_24"
                android:padding="8dp"
                android:hint="@string/edit_razon_social"
                android:textColor="@color/blue_progela"
                android:textColorHint="#C9C8CA"
                android:layout_marginTop="10dp"
                android:inputType="textAutoComplete"
                android:maxLength="200"
                android:maxLines="1"
                android:singleLine="true"
                android:imeOptions="actionNext"
                />

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_margin="1dp"
                app:boxStrokeColor="#C9C8CA"
                app:hintTextColor="#C9C8CA"
                android:paddingTop="8dp"
                app:hintAnimationEnabled="false"
                app:hintEnabled="false">

                <AutoCompleteTextView
                    android:id="@+id/txtVialidad"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/custom_edit_text"
                    android:completionThreshold="1"
                    android:drawableStart="@drawable/baseline_moving_24"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:imeOptions="actionNext"
                    android:inputType="none"
                    android:maxLines="1"
                    android:paddingStart="8dp"
                    android:paddingBottom="12dp"
                    android:singleLine="true"
                    android:hint="@string/edit_vialidad"
                    android:textColor="@color/blue_progela"
                    android:textColorHint="#C9C8CA"
                    android:textSize="18sp"
                    tools:ignore="RtlSymmetry" />
            </com.google.android.material.textfield.TextInputLayout>





            <EditText
                android:id="@+id/txtCalle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="-7dp"
                android:background="@drawable/custom_edit_text"
                android:drawableStart="@drawable/baseline_signpost_24"
                android:hint="@string/edit_calle"
                android:imeOptions="actionNext"
                android:inputType="textAutoComplete"
                android:maxLines="1"
                android:padding="8dp"
                android:singleLine="true"
                android:textColor="@color/blue_progela"
                android:textColorHint="#C9C8CA" />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_horizontal">

                <EditText
                    android:id="@+id/txtManzana"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10sp"
                    android:background="@drawable/custom_edit_text"
                    android:drawableStart="@drawable/baseline_place_24"
                    android:hint="@string/edit_manzana"
                    android:inputType="textAutoComplete"
                    android:maxLength="100"
                    android:maxLines="1"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/blue_progela"
                    android:textColorHint="#C9C8CA"
                    android:imeOptions="actionNext"/>

                <EditText
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:id="@+id/txtLote"
                    android:background="@drawable/custom_edit_text"
                    android:drawableStart="@drawable/baseline_place_24"
                    android:padding="8dp"
                    android:textColor="@color/blue_progela"
                    android:textColorHint="#C9C8CA"
                    android:hint="@string/edit_lote"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10sp"
                    android:inputType="textAutoComplete"
                    android:maxLength="100"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:imeOptions="actionNext"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <EditText
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:id="@+id/txtNumeroExterior"
                    android:background="@drawable/custom_edit_text"
                    android:drawableStart="@drawable/baseline_numbers_24"
                    android:padding="8dp"
                    android:textColor="@color/blue_progela"
                    android:textColorHint="#C9C8CA"
                    android:hint="@string/edit_numero_exterior"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10sp"
                    android:inputType="numberDecimal"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:imeOptions="actionNext"/>

                <EditText
                    android:id="@+id/txtNumeroInterior"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/custom_edit_text"
                    android:drawableStart="@drawable/baseline_numbers_24"
                    android:hint="@string/edit_numero_interior"
                    android:inputType="textAutoComplete"
                    android:maxLength="100"
                    android:maxLines="1"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/blue_progela"
                    android:textColorHint="#C9C8CA"  />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center">


                <Button
                    android:layout_width="150dp"
                    android:layout_height="60dp"
                    android:id="@+id/btnSiguiente"
                    android:textSize="18sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10sp"
                    android:text="@string/btn_prospectos_siguiente"
                    android:backgroundTint="@color/blue_progela"
                    app:cornerRadius="20dp"/>

            </LinearLayout>

        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>
